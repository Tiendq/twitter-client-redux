{
  "name": "twitter-client-redux",
  "version": "1.2.0",
  "description": "A Redux async action example with Twitter",
  "main": "index.js",
  "keywords": [
    "express",
    "react",
    "redux",
    "REST",
    "twitter"
  ],
  "scripts": {
    "build": "yarn build:client && yarn build:server",
    "build:client": "NODE_ENV=production webpack",
    "build:server": "BABEL_ENV=server babel src -d dist",
    "start": "nodemon src/index.js",
    "start:production": "NODE_ENV=production node dist/index.js",
    "test": "jest --config=jest.json --coverage",
    "test:watch": "jest --config=jest.json --watch",
    "watch": "webpack --mode=development --progress --colors --watch"
  },
  "repository": {
